<style>
    .heading{
    font-size: 20px;
    font-weight: bold;
    background: -webkit-linear-gradient(#2291af, #4bcef3);
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
}
.card1{
    background-color: #55cdef40;
}
.card2{
    background-color: #55cdef40;
}
.card1:hover {
    -webkit-transform: scale(1);
    transform: scale(1);
    background-color: #55ceef79;
}
.card2:hover {
    -webkit-transform: scale(1);
    transform: scale(1);
    background-color: #55ceef79;
}

