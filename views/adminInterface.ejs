        -webkit-box-sizing: content-box;
        -moz-box-sizing: content-box;
        box-sizing: content-box;
    }

    *:before,
    *:after {
        -webkit-box-sizing: content-box;
        -moz-box-sizing: content-box;
        box-sizing: content-box;
    }

    .table {
        --bs-table-striped-bg: #a5dae929;
        overflow: hidden;
    }

    .oneliner {
        overflow: hidden;
        white-space: nowrap;
        text-overflow: ellipsis;
