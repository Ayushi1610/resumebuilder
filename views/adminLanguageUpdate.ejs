<html>
<head>
    <link rel="stylesheet" type="text/css" href="/stylesheets/details.css">
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js" type="text/javascript"></script>
 </head>
 
 <body background="/images/background.jpg">
    <div class="root">
       
       <div style="margin-top: 2px;">
        <div style="display: flex;flex-direction: row;width: 100%;height: 70px;margin-bottom: 20px;">
            
            
        </div>
        <form action="/admin/adminLanguageUpdate" method="post" style="display:flex;justify-content: center;align-items: center;flex-direction:column;width: 100%;">
            <h2
                style="color: #4bcef3;margin-top: 20px;letter-spacing: 1;">
                Languages Known</h2>
                <input type="hidden" value="<%=userid%>" name="userid">
                    <input type="hidden" value="<%=aemail%>" name="aemail">
                    <input type="hidden" value="<%=apass%>" name="apass">
                    <input type="hidden" name="lid" value="<%=languagesid%>">
            
                
        
            <div style="display: flex;flex-direction: row;width:20%;margin-right: 10px;flex-wrap: wrap;">
               
               <input type="text" name="languages" value="<%=result.languages%>">
            </div>
           
            <input type="submit" value="Update">
        </div>
        </form>
    </div>
    </div>
 </body>
 </html>
 